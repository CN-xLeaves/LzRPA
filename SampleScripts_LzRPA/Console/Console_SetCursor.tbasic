Console_WaitKey 
Console_cls()

DisplayNumber()
Console_SetCursorPosition(10, 10)
Console_Write("Press any key for go to col 40, row 20") 
Console_WaitKey 
Console_cls()

DisplayNumber()
Console_SetCursorPosition(40, 20)
Console_Write("Press any key for go to col 22, row 24") 
Console_WaitKey 
